# Supabase Environment Variables
# Copy this file to .env and fill in the values

# Supabase Project URL (from Supabase dashboard or local: http://127.0.0.1:54321)
SUPABASE_URL=http://127.0.0.1:54321

# Supabase Anonymous Key (public, safe to expose to clients)
SUPABASE_ANON_KEY=your-anon-key

# Supabase Service Role Key (private, never expose to clients)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# SMS Provider (for OTP in production)
# TWILIO_ACCOUNT_SID=
# TWILIO_AUTH_TOKEN=
# TWILIO_PHONE_NUMBER=
